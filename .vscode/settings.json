{
  "elixirLS.dialyzerEnabled": true,
  "elixirLS.signatureAfterComplete": true,
  "elixirLS.incrementalDialyzer": true,
  "elixirLS.fetchDeps": false,
  "[elixir]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.wordBasedSuggestions": "off",
    "editor.formatOnType": true,
    "editor.trimAutoWhitespace": false,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "elixir-lsp.elixir-ls"
  },
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.useFlatConfig": true,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "eslint.rules.customizations": [
    {
      "rule": "style/*",
      "severity": "off"
    },
    {
      "rule": "format/*",
      "severity": "off"
    },
    {
      "rule": "*-indent",
      "severity": "off"
    },
    {
      "rule": "*-spacing",
      "severity": "off"
    },
    {
      "rule": "*-spaces",
      "severity": "off"
    },
    {
      "rule": "*-order",
      "severity": "off"
    },
    {
      "rule": "*-dangle",
      "severity": "off"
    },
    {
      "rule": "*-newline",
      "severity": "off"
    },
    {
      "rule": "*quotes",
      "severity": "off"
    },
    {
      "rule": "*semi",
      "severity": "off"
    }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "json",
    "jsonc",
    "yaml",
    "toml",
    "xml",
    "gql",
    "graphql",
    "astro"
  ],
  "cSpell.words": [
    "AFLAGS",
    "asyncify",
    "bcol",
    "bline",
    "bubbletea",
    "charmbracelet",
    "checkmark",
    "DEVBOX",
    "erlef",
    "Errorf",
    "esbuild",
    "Freqs",
    "fsys",
    "gofmt",
    "golangci",
    "golint",
    "gopls",
    "inconshreveable",
    "ints",
    "Javy",
    "kcol",
    "lipgloss",
    "lparen",
    "myaction",
    "myapp",
    "mycommand",
    "mycompany",
    "ncol",
    "nixpkgs",
    "paulhatch",
    "pflag",
    "PREOF",
    "rebar",
    "rparen",
    "rwxr",
    "scol",
    "simpleplatform",
    "sline",
    "softprops",
    "staticcheck",
    "tmpl",
    "userstamps",
    "vcol",
    "vitest",
    "vline"
  ]
}
